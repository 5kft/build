+++ b/arch/riscv/boot/dts/spacemit/k1-x_deb1.dts
@@ -11,7 +11,7 @@
 #include "k1-x-camera-sdk.dtsi"
 
 / {
-	model = "spacemit k1-x deb1 board";
+	model = "BananaPi BPI-F3";
 	modules_usrload = "8852bs";
 
 	cpus: cpus {
diff --git a/arch/riscv/boot/dts/spacemit/keystone-k1-bananapi-f3.dts b/arch/riscv/boot/dts/spacemit/keystone-k1-bananapi-f3.dts
new file mode 120000
index 000000000..1f6932c8e
--- /dev/null
+++ b/arch/riscv/boot/dts/spacemit/keystone-k1-bananapi-f3.dts
@@ -0,0 +1 @@
+k1-x_deb1.dts
\ No newline at end of file
-- 
